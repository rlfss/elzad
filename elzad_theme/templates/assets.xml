<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend_elzad" inherit_id="website.assets_frontend" name="Website Assets">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/variable.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/button.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/form.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/login.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/signup.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/global.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/pagination.scss"/>

            <!-- Owl Stylesheets -->
            <link rel="stylesheet" href="/elzad_theme/static/src/css/owl.carousel.min.css"/>
            <link rel="stylesheet" href="/elzad_theme/static/src/css/owl.theme.default.min.css"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/header.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/footer.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/home.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/shop.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/product_details.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/contactus.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/cart.scss"/>
            <link rel="stylesheet" type="text/scss" href="/elzad_theme/static/src/scss/home-ar.scss" t-if="request.env.lang=='ar_AA' or request.env.lang=='ar_SY'"/>
        </xpath>
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/elzad_theme/static/src/js/owl.carousel.min.js"></script>
            <script type="text/javascript" src="/elzad_theme/static/src/js/script-en.js" t-if="request.env.lang!=ar_AA or request.env.lang==ar_SY"/>
            <script type="text/javascript" src="/elzad_theme/static/src/js/script-ar.js" t-if="request.env.lang==ar_AA or request.env.lang==ar_SY"/>
            <script type="text/javascript" src="/elzad_theme/static/src/js/signup.js"/>
            <script type="text/javascript" src="/elzad_theme/static/src/js/script.js"/>
        </xpath>
    </template>
</odoo>